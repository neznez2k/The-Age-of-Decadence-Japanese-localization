      <answers>
        <dlgAnsw>
          <text>"I hear you've been trading with the Aurelians down at the mine."</text>
          <def_link>2595</def_link>
          <checkOnAppear />
          <checksOnClick />
          <linksOnClick />
          <scriptsOnClick />
