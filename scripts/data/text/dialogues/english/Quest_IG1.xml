        <string>dlgFadeToThisPart();</string>
        <string>dlgCreateCharacter("Recruit", 1512, 1512, 221, 52, 0, N);</string>
        <string>dlgCreateCharacter("Recruit", 1513, 1513, 224, 52, 0, N);</string>
        <string>dlgTeleportTo(222, 54, 0, S, 1130);</string>
      </onLoadScripts>
      <answers>
        <dlgAnsw>
          <text>Continue.</text>
          <def_link>4</def_link>
          <checkOnAppear />
