    <dlgPart>
      <portrait />
      <speaker_name />
      <npc_text>[failure]"I thought you served Antidas."</npc_text>
      <onLoadScripts />
      <answers>
        <dlgAnsw>
          <text>[streetwise]"I've lost faith in Lord Antidas. His domain is dwindling, while Lord Gaelius' domain and influence are growing. I daresay I'm not the first to look for a more suitable master."</text>
          <def_link>1813</def_link>
          <checkOnAppear />
          <checksOnClick>
            <string>aod.streetwise &gt;= 3</string>
          </checksOnClick>
          <linksOnClick>
            <int>1814</int>
          </linksOnClick>
          <scriptsOnClick />
        </dlgAnsw>
        <dlgAnsw>
          <text>[charisma]"I thought you served Caer-Tor."</text>
