      <answers />
      <UID>9297</UID>
    </dlgPart>
    <dlgPart>
      <portrait />
      <speaker_name />
      <npc_text>"What are we waiting for?"</npc_text>
      <onLoadScripts>
        <string>dlgCam(345, 40, 7, 1060);</string>
        <string>dlgTeleportToMarker(TM_DLG_Antidas);</string>
      </onLoadScripts>
      <answers>
        <dlgAnsw>
          <text>Travel to the temple.</text>
          <def_link>9299</def_link>
          <checkOnAppear />
          <checksOnClick>
            <string>aod.mdn_destroyed || aod.mdn_slumsAttacked</string>
          </checksOnClick>
          <linksOnClick>
            <int>9300</int>
          </linksOnClick>
          <scriptsOnClick />
        </dlgAnsw>
        <dlgAnsw>
          <text>Check your gear one last time.</text>
          <def_link>9297</def_link>
          <checkOnAppear />
          <checksOnClick />
          <linksOnClick />
          <scriptsOnClick />
        </dlgAnsw>
      </answers>
      <UID>9298</UID>
    </dlgPart>
