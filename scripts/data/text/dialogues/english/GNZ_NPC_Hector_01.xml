          <checksOnClick />
          <linksOnClick />
          <scriptsOnClick />
        </dlgAnsw>
        <dlgAnsw>
          <text>“He sent me to you, so it’s for you to decide.”</text>
          <def_link>4</def_link>
          <checkOnAppear />
