          <checkOnAppear />
          <checksOnClick />
          <linksOnClick />
          <scriptsOnClick />
        </dlgAnsw>
      </answers>
      <UID>2</UID>
    </dlgPart>
    <dlgPart>
      <portrait />
      <speaker_name />
      <npc_text>"Then you'll know what I know. Each man you kill teaches you something."</npc_text>
