    <dlgPart>
      <portrait />
      <speaker_name />
      <npc_text>END - explore the Hangar</npc_text>
      <onLoadScripts>
        <string>dlgFinish();</string>
        <string>dlgFadeShortToThisPart();</string>
      </onLoadScripts>
      <answers />
