    <dlgPart>
      <portrait />
      <speaker_name />
      <npc_text>「何かご用かな？」</npc_text>
      <onLoadScripts>
        <string>aod.npc_teronBlacksmith_met = true;</string>
        <string>dlgTeleportToMarker(TM_Blacksmith_NPC_01, 1900);</string>
        <string>dlgTeleportToMarker(TM_Blacksmith_PC_01);</string>
