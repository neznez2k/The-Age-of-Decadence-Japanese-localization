      </onLoadScripts>
      <answers>
        <dlgAnsw>
          <text>“Yet you gave Ganezzar to Maseus.”</text>
          <def_link>1</def_link>
          <checkOnAppear />
          <checksOnClick />
          <linksOnClick />
          <scriptsOnClick />
        </dlgAnsw>
        <dlgAnsw>
          <text>“Thank you, master Strabos.”</text>
