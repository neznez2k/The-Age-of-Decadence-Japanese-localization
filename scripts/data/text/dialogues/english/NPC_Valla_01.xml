        <string>dlgFadeShortToThisPart();</string>
        <string>Door_AmeriusHouse.open();</string>
        <string>dlgTeleportToMarker(TM_AmeriusHouse_06, 4927);</string>
      </onLoadScripts>
      <answers>
        <dlgAnsw>
          <text>"I want to talk to Marcus Valla."</text>
          <def_link>20</def_link>
          <checkOnAppear />
          <checksOnClick />
          <linksOnClick />
          <scriptsOnClick />
        </dlgAnsw>
      </answers>
      <UID>21</UID>
