    <dlgPart>
      <portrait />
      <speaker_name />
      <npc_text>“I certainly do. So, who said yes to you?”</npc_text>
      <onLoadScripts />
      <answers>
        <dlgAnsw>
          <text>“Lord Serenas.”</text>
          <def_link>4758</def_link>
          <checkOnAppear />
          <checksOnClick />
          <linksOnClick />
          <scriptsOnClick />
        </dlgAnsw>
        <dlgAnsw>
