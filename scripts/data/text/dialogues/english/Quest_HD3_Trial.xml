          <checkOnAppear />
          <checksOnClick />
          <linksOnClick />
          <scriptsOnClick />
        </dlgAnsw>
        <dlgAnsw>
          <text>“Lady Lorenza.”</text>
          <def_link>4768</def_link>
          <checkOnAppear />
          <checksOnClick />
          <linksOnClick />
          <scriptsOnClick />
        </dlgAnsw>
      </answers>
      <UID>4757</UID>
